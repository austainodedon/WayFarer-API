{
  "name": "wayfarer-api",
  "version": "1.0.0",
  "engines": {
    "node": "10.13.0"
  },
  "description": "WayFarer is a public bus transportation booking server. This repository shows how to develop the back-end API",
  "main": "server.js",
  "scripts": {
    "heroku-postbuild": "npm install --production",
    "test": "nyc mocha --require babel-register --require babel-polyfill dist/tests/users.test.js --exit",
    "build": "babel backend --out-dir build",
    "start": "node build backend/server.js",
    "start:dev": "set DEBUG=dev & nodemon --exec babel-node backend/server.js",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "dist": "babel --require babel-polyfill backend -d dist",
    "db": "babel-node ./backend/model/conn"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/austainodedon/WayFarer-API.git"
  },
  "keywords": [
    "wayfarer",
    "api",
    "share",
    "ride"
  ],
  "author": "Augustine Emeka",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/austainodedon/WayFarer-API/issues"
  },
  "homepage": "https://github.com/austainodedon/WayFarer-API#readme",
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.0",
    "@babel/node": "^7.5.0",
    "@babel/preset-env": "^7.5.3",
    "@babel/register": "^7.4.4",
    "babel-register": "^6.26.0",
    "chai-http": "^4.3.0",
    "coveralls": "^3.0.4",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.2.0",
    "eslint-plugin-import": "^2.18.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "nyc": "^14.1.1"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "bcrypt": "^3.0.6",
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.4",
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "joi": "^14.3.1",
    "jsonwebtoken": "^8.5.1",
    "make-runnable": "^1.3.6",
    "moment": "^2.24.0",
    "morgan": "^1.9.1",
    "pg": "^7.11.0"
  }
}
